
public class Elf extends Actor{
	
	protected char actorType = ' ';
	protected int strength;
	private int magic;
	
	// constructors
	public Elf ( ){
		
	}
	
	public Elf(Actor actor ){
		super (actor);
	}
	
	public Elf(char actorType, int strength, int magic) {
		super (actorType, strength);
		this.strength *= magic; 
	}


}
